{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_expo/*", "/assets/*", "*.{css,js,json,png,jpg,svg,ico,woff,woff2}"]
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".woff2": "font/woff2"
  }
}